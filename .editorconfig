# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
charset = utf-8

[Makefile]
indent_style = tab

[*.yml]
indent_size = 2

[*.feature]
indent_size = 4
indent_style = space
